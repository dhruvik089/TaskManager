@model List<TaskManagement.Models.ViewModel.TaskList>

<!--*******************
    Preloader start
********************-->
<div id="preloader">
    <div class="loader">
        <svg class="circular" viewBox="25 25 50 50">
            <circle class="path" cx="50" cy="50" r="20" fill="none" stroke-width="3" stroke-miterlimit="10" />
        </svg>
    </div>
</div>
<!--*******************
    Preloader end
********************-->
<!--**********************************
    Main wrapper start
***********************************-->
<div id="main-wrapper">

    <!--**********************************
        Nav header start
    ***********************************-->
    <div class="nav-header">
        <div class="brand-logo">
            <a href="index.html">
                <b class="logo-abbr"><img src="images/logo.png" alt=""> </b>
                <div class="brand-title" />
                <img src="~/Content/Theme/TeacherDashBoard/images/logo-text.png" />
        </div>
        </a>
    </div>
</div>
<!--**********************************
    Nav header end
***********************************-->
<!--**********************************
    Header start
***********************************-->
<div class="header">
    <div class="header-content clearfix">

        <div class="nav-control">
            <div class="hamburger">
                <span class="toggle-icon"><i class="icon-menu"></i></span>
            </div>
        </div>

        <div class="header-right">

            <div class="d-flex">
                <h4 class="mt-3">
                    @Session["Teachername"]

                </h4>
                <button class="btn">

                    @Html.ActionLink("Log Out", "Login", controllerName: "LoginSignup")
                </button>
            </div>

        </div>
    </div>
</div>
<!--**********************************
    Header end ti-comment-alt
***********************************-->
<!--**********************************
    Sidebar start
***********************************-->
<div class="nk-sidebar">
    <div class="nk-nav-scroll">
        <ul class="metismenu" id="menu">
            <li class="nav-label">Dashboard</li>
            <li>
                <a class="has-arrow" href="javascript:void()" aria-expanded="false">
                    <i class="icon-speedometer menu-icon"></i><span class="nav-text">Task</span>
                </a>
                <ul aria-expanded="false">
                    <li>@Html.ActionLink("Create Task", "CreateTask")</li>
                    <li>@Html.ActionLink("Assign Task", "AsignTask")</li>
                    <!-- <li><a href="./index-2.html">Home 2</a></li> -->
                </ul>
            </li>
        </ul>
    </div>
</div>
<!--**********************************
    Sidebar end
***********************************-->

<div class="content-body">
    <div class="container">
        <div class="container">
            <h3 class="text-center">All Complete Task </h3>

            <table class="table">
                <thead>
                    <tr>
                        <th>TaskID</th>
                        <th>Student Name</th>
                        <th>Task Name</th>
                        <th>Description</th>
                        <th>Action</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>@item.TaskID</td>
                            <td>@item.Username</td>
                            <td>@item.TaskName</td>
                            <td>@item.Description</td>
                            <td>
                                <button class="btn btn-info">Accept</button>
                                <button class="btn btn-danger">Reject</button>
                            </td>
                        </tr>
                    }
                </tbody>
            </table>

            <button class="btn btn-primary">
                @Html.ActionLink("back to Home", "Teacher", "Teacher", htmlAttributes: new { @class = "text-decoration-none text-white" })
            </button>

        </div>
    </div>
</div>

<!--**********************************
      Footer start
  ***********************************-->
<div class="footer">
    <div class="copyright">
        <p>Copyright &copy; Designed & Developed by <a href="https://themeforest.net/user/quixlab">Quixlab</a> 2018</p>
    </div>
</div>
<!--**********************************
    Footer end
***********************************-->
